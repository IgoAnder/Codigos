#include <stdio.h>
// Digitar uma matriz 3x3 e mostrar a soma de seus elementos
int main()
{
    int i, j, m[3][3], soma;
    soma = 0;
    for (i = 0; i < 3; i++)
    {
        for (j = 0; j < 3; j++)
        {
            printf("Digite o elemento da linha %d e da coluna %d da matriz:", i + 1, j + 1);
            scanf("%d", &m[i][j]);
            soma = soma + m[i][j];
        }
    }
    printf("A soma dos elementos da matriz digitada foi: %d", soma);
    return 0;
}
